<?xml version="1.0"?>
<component name="org.nuxeo.web.ui.properties.contrib">
  <extension target="org.nuxeo.runtime.ConfigurationService" point="configuration">
    <!-- enrichers -->
    <property name="org.nuxeo.web.ui.enrichers" list="true">hasContent</property>
    <property name="org.nuxeo.web.ui.enrichers">firstAccessibleAncestor</property>
    <property name="org.nuxeo.web.ui.enrichers">permissions</property>
    <property name="org.nuxeo.web.ui.enrichers">breadcrumb</property>
    <property name="org.nuxeo.web.ui.enrichers">preview</property>
    <property name="org.nuxeo.web.ui.enrichers">favorites</property>
    <property name="org.nuxeo.web.ui.enrichers">subscribedNotifications</property>
    <property name="org.nuxeo.web.ui.enrichers">thumbnail</property>
    <property name="org.nuxeo.web.ui.enrichers">renditions</property>
    <property name="org.nuxeo.web.ui.enrichers">pendingTasks</property>
    <property name="org.nuxeo.web.ui.enrichers">runnableWorkflows</property>
    <property name="org.nuxeo.web.ui.enrichers">runningWorkflows</property>
    <property name="org.nuxeo.web.ui.enrichers">collections</property>
    <property name="org.nuxeo.web.ui.enrichers">audit</property>
    <property name="org.nuxeo.web.ui.enrichers">subtypes</property>
    <property name="org.nuxeo.web.ui.enrichers">tags</property>
    <property name="org.nuxeo.web.ui.enrichers">publications</property>

    <!-- Properties to be fetched when loading current document, default is 'properties' meaning all -->
    <property name="org.nuxeo.web.ui.fetchedProperties">properties</property>

    <!-- Date (time) format -->
    <property name="org.nuxeo.web.ui.dateFormat">LL</property>
    <property name="org.nuxeo.web.ui.dateTimeFormat">LLL</property>
  </extension>
</component>
